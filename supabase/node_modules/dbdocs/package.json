{
  "name": "dbdocs",
  "version": "0.8.4",
  "author": "@holistics",
  "bin": {
    "dbdocs": "./bin/run"
  },
  "resolutions": {
    "oclif/**/ansi-regex": "^3.0.1"
  },
  "dependencies": {
    "@dbml/core": "^2.6.0",
    "@oclif/core": "1.12.1",
    "@oclif/plugin-help": "5.1.12",
    "axios": "0.27.2",
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "inquirer": "^7.0.1",
    "netrc-parser": "^3.1.6",
    "open": "^7.0.0",
    "ora": "^4.0.3",
    "remove-markdown": "^0.5.0",
    "update-notifier": "^6.0.2"
  },
  "devDependencies": {
    "@oclif/test": "2.1.1",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^10.2.0",
    "nyc": "^14",
    "oclif": "^3.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "additionalHelpFlags": [
      "help"
    ],
    "additionalVersionFlags": [
      "version"
    ],
    "commands": "./src/commands",
    "bin": "dbdocs",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./src/hooks/init/checkUpdate",
        "./src/hooks/init/loadEnv"
      ],
      "prerun": [
        "./src/hooks/prerun/checkVersion"
      ]
    }
  },
  "repository": "holistics/dbdocs",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif manifest",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif readme && git add README.md"
  }
}
